<div class="row">
    <div class="col-md-6 order-md-2 mb-4">
        <h4 class="d-flex justify-content-between align-items-center mb-3">
        <span class="text-muted">User Env</span>
        </h4>
        <ul class="list-group mb-3">
        <li class="list-group-item d-flex justify-content-between lh-condensed">
            <div>
            <h6 class="my-0">Profile API</h6>
                <small class="text-muted"> {{ userenv.profileapi_key }} </small>
            </div>
            {{!-- <span class="text-muted">$12</span> --}}
        </li>
        <li class="list-group-item d-flex justify-content-between lh-condensed">
            <div>
            <h6 class="my-0">Access Token</h6>
            <small class="text-muted">{{ userenv.access_token }}</small>
            </div>
            {{!-- <span class="text-muted">$8</span> --}}
        </li>
        <li class="list-group-item d-flex justify-content-between lh-condensed">
            <div>
            <h6 class="my-0">Page Token</h6>
            <small class="text-muted">{{ userenv.page_token }}</small>
            </div>
            {{!-- <span class="text-muted">$5</span> --}}
        </li>
        <li class="list-group-item d-flex justify-content-between lh-condensed">
            <div>
            <h6 class="my-0">Status</h6>
            <small class="text-muted">{{ userenv.status }}</small>
            </div>
            {{!-- <span class="text-muted">$5</span> --}}
        </li>
        </ul>

    </div>
    <div class="col-md-6 order-md-1">
        <h4 class="mb-3">Basic Information</h4>
        <form class="needs-validation" novalidate="">
        <div class="row">
            <div class="col-md-12 mb-3">
            <label for="name">Name</label>
            <input type="text" class="form-control" id="displayName" placeholder="" value="{{user.name}}" readonly>
            
            </div>
            <div class="col-md-12 mb-3">
            <label for="email">Email</label>
            <input type="text" class="form-control" id="email" placeholder="" value="{{user.email}}" readonly>
            </div>
        </div>


        <hr class="mb-4">
        <a class="btn btn-primary btn-lg btn-block" href="/edit/{{ user._id }}">Click to Edit</a>

        </form>
    </div>

</div>